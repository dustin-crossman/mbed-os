{
    "GCC_ARM": {
        "common": ["-c", "-Wall", "-Wextra",
                   "-Wno-unused-parameter", "-Wno-missing-field-initializers",
                   "-fmessage-length=0", "-fno-exceptions", "-fno-builtin",
                   "-ffunction-sections", "-fdata-sections", "-funsigned-char",
                   "-MMD", "-fno-delete-null-pointer-checks",
                   "-fomit-frame-pointer", "-O0", "-g3", "-DMBED_DEBUG",
                   "-DMBED_TRAP_ERRORS_ENABLED=1",
                   "-DMBED_CONF_PLATFORM_DEFAULT_SERIAL_BAUD_RATE=9600",
                   "-DMBED_CONF_PLATFORM_CTHUNK_COUNT_MAX=8",
                   "-Werror",
                   "-Icmsis", "-Icmsis/TARGET_CORTEX_M",
                   "-IUNITTESTS/target_h", 
                   "-Ihal",  
                   "-Iusb/device/USBDevice",  
                   "-Iusb/device/USBPhy", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6csp/core_lib/include", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6csp/hal/include", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6pdl/drivers/include",  
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6pdl/devices/include",  
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT/device", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT/SDIO_HOST",
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6", 
                   "-Iplatform"],
        "asm": ["-x", "assembler-with-cpp"],
        "c": ["-std=gnu99"],
        "cxx": ["-std=gnu++98", "-fno-rtti", "-Wvla"],
        "ld": ["-Wl,--gc-sections", "-Wl,--wrap,main", "-Wl,--wrap,_malloc_r",
               "-Wl,--wrap,_free_r", "-Wl,--wrap,_realloc_r", "-Wl,--wrap,_memalign_r",
               "-Wl,--wrap,_calloc_r", "-Wl,--wrap,exit", "-Wl,--wrap,atexit",
               "-Wl,-n"]
    },
    "ARMC6": {
        "common": ["-c", "--target=arm-arm-none-eabi", "-mthumb", "-g", "-O1",
                   "-Wno-armcc-pragma-push-pop", "-Wno-armcc-pragma-anon-unions",
                   "-DMULADDC_CANNOT_USE_R7", "-fdata-sections",
                   "-fno-exceptions", "-MMD", "-D_LIBCPP_EXTERN_TEMPLATE(...)=",
                   "-fshort-enums", "-fshort-wchar",
                   "-DMBED_CONF_PLATFORM_DEFAULT_SERIAL_BAUD_RATE=9600",
                   "-DMBED_CONF_PLATFORM_CTHUNK_COUNT_MAX=8",
                   "-Werror",
                   "-Icmsis", "-Icmsis/TARGET_CORTEX_M",
                   "-Ihal",  
                   "-Iusb/device/USBDevice",  
                   "-Iusb/device/USBPhy", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6csp/core_lib/include", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6csp/hal/include", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6pdl/drivers/include",  
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6pdl/devices/include",  
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT/device", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT/SDIO_HOST", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6", 
                   "-Iplatform"],
        "asm": [],
        "c": ["-D__ASSERT_MSG", "-std=gnu99"],
        "cxx": ["-fno-rtti", "-std=gnu++98"],
        "ld": ["--verbose", "--remove", "--show_full_path", "--legacyalign"]
    },
    "ARM": {
        "common": ["-c", "--gnu", "-Otime", "--split_sections",
                   "--apcs=interwork", "--brief_diagnostics", "--restrict",
                   "--multibyte_chars", "-O0", "-g", "-DMBED_DEBUG",
                   "-DMBED_TRAP_ERRORS_ENABLED=1",
                   "-DMBED_CONF_PLATFORM_DEFAULT_SERIAL_BAUD_RATE=9600",
                   "-DMBED_CONF_PLATFORM_CTHUNK_COUNT_MAX=8",
                   "--diag_error=warning",
                   "-Icmsis", "-Icmsis/TARGET_CORTEX_M",
                   "-Ihal",  
                   "-Iusb/device/USBDevice",  
                   "-Iusb/device/USBPhy", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6csp/core_lib/include", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6csp/hal/include", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6pdl/drivers/include",  
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6pdl/devices/include",  
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT/device", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT/SDIO_HOST", 
                   "-Itargets/TARGET_Cypress/TARGET_PSOC6", 
                   "-Iplatform"],
        "asm": [],
        "c": ["--md", "--no_depend_system_headers", "--c99", "-D__ASSERT_MSG"],
        "cxx": ["--cpp", "--no_rtti", "--no_vla"],
        "ld": ["--show_full_path"]
    },
    "uARM": {
        "common": ["-c", "--gnu", "-Otime", "--split_sections",
                   "--apcs=interwork", "--brief_diagnostics", "--restrict",
                   "--multibyte_chars", "-O0", "-D__MICROLIB", "-g",
                   "--library_type=microlib", "-DMBED_RTOS_SINGLE_THREAD", "-DMBED_DEBUG",
                   "-DMBED_TRAP_ERRORS_ENABLED=1"],
        "asm": [],
        "c": ["--md", "--no_depend_system_headers", "--c99", "-D__ASSERT_MSG"],
        "cxx": ["--cpp", "--no_rtti", "--no_vla"],
        "ld": ["--library_type=microlib"]
    },
    "IAR": {
        "common": [
            "--no_wrap_diagnostics",  "-e",
            "--diag_suppress=Pa050,Pa084,Pa093,Pa082", "-On", "-r", "-DMBED_DEBUG",
            "-DMBED_TRAP_ERRORS_ENABLED=1", "--enable_restrict",
            "-DMBED_CONF_PLATFORM_DEFAULT_SERIAL_BAUD_RATE=9600",
            "-DMBED_CONF_PLATFORM_CTHUNK_COUNT_MAX=8",
            "--error_limit=10000", "--warnings_are_errors",
            "-Icmsis", "-Icmsis/TARGET_CORTEX_M",
            "-Ihal",  
            "-Iusb/device/USBDevice",  
            "-Iusb/device/USBPhy", 
            "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6csp/core_lib/include", 
            "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6csp/hal/include", 
            "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6pdl/drivers/include",  
            "-Itargets/TARGET_Cypress/TARGET_PSOC6/psoc6pdl/devices/include",  
            "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT", 
            "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT/device", 
            "-Itargets/TARGET_Cypress/TARGET_PSOC6/TARGET_CY8CKIT_062_WIFI_BT/SDIO_HOST", 
            "-Itargets/TARGET_Cypress/TARGET_PSOC6", 
            "-Iplatform"],
        "asm": [],
        "c": ["--vla"],
        "cxx": ["--guard_calls", "--no_static_destruction"],
        "ld": ["--skip_dynamic_initialization", "--threaded_lib"]
    }
}
